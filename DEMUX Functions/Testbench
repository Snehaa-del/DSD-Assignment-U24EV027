module demux1x4_func_testbench;
  reg I;
  reg [1:0] s;
  wire [3:0] Y;

  demux1x4_func uut (.I(I), .s(s), .Y(Y));

  initial begin
    $dumpfile("demux1X_func.vcd");
    $dumpvars(0, demux1x4_func_testbench);
    $display("s  I | Y");
    $monitor("%b   %b | %b", s, I, Y);

    I = 1; s = 2'b00; #10;
    I = 1; s = 2'b01; #10;
    I = 1; s = 2'b10; #10;
    I = 1; s = 2'b11; #10;
  end
endmodule
